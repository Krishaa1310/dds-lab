#include <iostream>
using namespace std;

int A[8]={9,3,7,1,5,6,2,4};
int n = 8;

void swap(int p , int q){
    int t;
    t = A[p];
    A[p]=A[q];
    A[q]=t;
}
void show(){
    int k;
    for(k=0;k<=n-1;k++)
    cout<<A[k]<<"\t"<<endl;
    cout<<"\n";
}
int main() {
    int i,j;
    show();
    for(i=0;i<=n-2;i++){
        for(j=i+1;j<=n-1;j++){
            if(A[i]>A[j]){
                swap(i,j);
            }
        }
      
    }
    show();
    return 0;

}
